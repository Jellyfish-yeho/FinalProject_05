<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<!-- 자주 사용하는 type의 별칭을 등록해 놓고 Mapper.xml에서 사용할 수 있다. -->
	<typeAliases>

		<typeAlias type="com.minton.minton05.users.dto.UsersDto" alias="usersDto"/>   
		<typeAlias type="com.minton.minton05.gallery.dto.GalleryDto" alias="galleryDto"/>
		<typeAlias type="com.minton.minton05.cafe.dto.CafeDto" alias="cafeDto"/>
		<typeAlias type="com.minton.minton05.cafe.dto.CafeCommentDto" alias="cafeCommentDto"/>
		<typeAlias type="com.minton.minton05.notice.dto.NoticeDto" alias="noticeDto"/>

	</typeAliases>
	
	<!-- 필요한 mapper xml 등록하기 -->
	<mappers>
    <mapper resource="com/minton/minton05/myBatis/UsersMapper.xml"/>
		<mapper resource="com/minton/minton05/myBatis/GalleryMapper.xml"/>
		<mapper resource="com/minton/minton05/myBatis/CafeMapper.xml"/>
		<mapper resource="com/minton/minton05/myBatis/CafeCommentMapper.xml"/>
		<mapper resource="com/minton/minton05/myBatis/NoticeMapper.xml"/>

	</mappers>
</configuration>